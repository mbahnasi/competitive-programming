#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
/*  اللهم اجعل هذا في سبيلك  */

//link:https://codeforces.com/contest/366/problem/C
// the basic thinking here is to make a dp[n][max sum of a][max sum of b] but this can`t be applied due to memory
// so you need to find a trick to handle that the(sum of a / sum of b =k)
// the trick here is to use the difference : ( sum of a = k * sum of b) for the needed subseq
// so, sum of a - k * sum of b =0
// so, we can make the dp on the difference. Another problem is that the difference can be less than 0
// so we need to control it to not go under zero by :
// I used a custom sort to sort the input decreasingly by differece so I can avoid this case, which is not the best solution but worked
// another solution is to say dp[i][dif + 1e5] shift the whole working 1e5 forword as this can handle the problem of zero at these constraints
//(shifting the indicies by the absolute value biggest negative number of difference)


const int N = 105 ;
int n,k, a[N],b[N], dp[N][100005], idx[N];
int maxTaste(int i, int dif) {
    // base case:
    if (i == n) {
        return dif == 0 ? 0 : -1e9 ;
    }
    // dp magic:
    int &dpp = dp[idx[i]][dif];
    if (dpp != -1) return dpp ;
    //transition:
    int ch1 = -1e9;
    if (dif + a[idx[i]] - k*b[idx[i]] >= 0 )ch1 = a[idx[i]] + maxTaste(i+1, dif + a[idx[i]] - k*b[idx[i]]);
    int ch2 = maxTaste(i+1, dif) ;
    return dpp = max(ch1, ch2) ;
}

void solve() {
    cin >> n >> k ;
    for (int i=0 ;i<n ;i++)cin >> a[i] ;
    for (int i=0 ;i<n ;i++)cin >> b[i] ;
    iota(idx, idx +n, 0 ) ;
    sort(idx, idx+n, [&](int i, int j) {
       return a[i] - k* b[i] > a[j] - k*b[j] ;
    });
    memset(dp, -1,sizeof(dp)) ;
    int ans = maxTaste(0,0) ;
    cout << (ans <= 0  ? -1 : ans) ;
}

int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
#ifndef ONLINE_JUDGE
    freopen("input.txt", "r",stdin);
    freopen("output.txt", "w",stdout);
#endif
    int tc;
    //cin >> tc ;
    tc = 1;
    while (tc--) solve();
}
